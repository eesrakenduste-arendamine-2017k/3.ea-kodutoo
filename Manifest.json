{
  "name": "Novelupdates",
  "description": "Minu Reading Listi RSS Feed",
  "icons": {	"16": "favicon.ico",
				"48": "icon.png",
				"128": "icon.png" },
  "version": "1.0",
  "permissions": [
    "activeTab",
	"tabs",
	"http://*/*",
	"https://*/*",
	"http://www.google.com/",
	"webNavigation",
	"notifications",
	"http://www.novelupdates.com/rss.php?uid=17670&unq=56ea8c3a00624&type=read"
  ],
 
  "content_scripts" : [{
        "matches" : [ "*://*/*"],
        "js" : ["background.js"],
        "css" : ["styles.css"]
    }],
  "browser_action": {
    "default_title": "Novel Feed",
	"default_icon": "icon.png",
	"default_popup": "popup.html"
  },
  "manifest_version": 2
}