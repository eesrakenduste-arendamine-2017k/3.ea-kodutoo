{
  "name": "StudyBuddy",
  "description": "Study Buddy",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts" : [{
        "matches" : [ "http://*/*","https://*/*"],
        "js" : ["extension.js"]
        
  }],
  "browser_action": {
    "default_title": "StudyBuddy",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}