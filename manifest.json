{
	// Name, description, version
	"name": "Multiple Tab Opener",
	"description": "This extension will allow you to open a bulk of links at once, all in their own tabs.",
	"version": "1.0",
	// Manifest version
	"manifest_version": 2,
	// Browser action
	"browser_action":{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	// Permissions
	"permissions": [
		"tabs",
		"storage"
	]
}